{
	"name": "csvFiletoSQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "SourceCSVFile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_output",
						"type": "DatasetReference"
					},
					"name": "SinkSQL"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          StudentID as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Age as string,",
				"          Gender as string,",
				"          Class as string,",
				"          Marks as string,",
				"          Email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceCSVFile",
				"SourceCSVFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          StudentID as integer,",
				"          FristName as string,",
				"          LastName as string,",
				"          Age as integer,",
				"          Gender as string,",
				"          Class as string,",
				"          Marks as integer,",
				"          Email as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SinkSQL"
			]
		}
	}
}